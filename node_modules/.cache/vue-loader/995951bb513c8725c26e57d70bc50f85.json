  },\n\n        checkout() {\n           this.orderChecked = true;\n           this.$store.dispatch('CHECKOUT', this.orders);\n        },\n\n        closePopup() {\n            this.orderChecked = false;\n        },\n\n        removeFromCart(index) {\n            this.$store.dispatch('REMOVE_FROM_CART', index);\n        },\n\n        plusQty(index) {\n            this.$store.dispatch('INCREMENT_PRODUCT_QTY', index);\n        },\n\n        minusQty(index) {\n            this.$store.dispatch('DECREMENT_PRODUCT_QTY', index);\n        }\n    },\n    created() {\n        this.orderChecked = false;\n    },\n\n    mounted() {\n        this.$store.dispatch('SET_HEADER_TEXT', 'Мои заказы');\n    },\n}\n",{"version":3,"sources":["a-order-list.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAsCA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA","file":"a-order-list.vue","sourceRoot":"src/components","sourcesContent":["<template>\n    <div class=\"a-order-list\">\n        <a-order-item\n            :order_data=\"item\"\n            v-for=\"(item, index) in orders\"\n            :key=\"item.id\"\n            @remove_item=\"removeFromCart(index)\"\n            @plus_qty=\"plusQty(index)\"\n            @minus_qty=\"minusQty(index)\"\n        />\n        <div class=\"order-total bg-color2 fixed-bottom\" v-if=\"orders.length > 0\">\n            <div class=\"total-sum\">\n                <div>Итого:</div>\n                <span>{{ totalSum }} руб.</span>\n            </div>\n            <p class=\"total-sum_confirm btn\" @click=\"checkout\">Оформить заказ</p>\n        </div>\n        <div class=\"empty-order-list\" v-if=\"orders.length < 1\">\n            <p class=\"empty-order-list-p\">У вас нет активных заказов</p>\n            <router-link to=\"/sellers-list\">\n                <p class=\"btn bg-color\" @click=\"sellersPageClick\">Добавить</p>\n            </router-link>\n        </div>\n        <transition name=\"bounce\">\n            <div class=\"thankyou-popup\" v-if=\"orderChecked\">\n            <p>Ваш заказ оформлен!</p>\n            <br>\n            <span>Представитель поставщика свяжется с вами в ближайшее время.</span>\n            <br>\n            <br>\n            <br>\n                <div class=\"ok btn bg-color\" @click=\"closePopup\">OK</div>\n        </div>\n        </transition>\n    </div>\n</template>\n\n<script>\n    import aOrderItem from '@/components/a-order-item'\n\n    export default {\n        name: \"a-order-list\",\n        components: {\n            aOrderItem\n        },\n        data() {\n            return {\n                orderChecked: false\n            }\n        },\n        computed: {\n            orders() {\n                return this.$store.state.cart;\n            },\n\n            totalSum() {\n                let totalSum = 0;\n                for (let i = 0; i< this.orders.length; i++) {\n                    totalSum += parseInt(this.orders[i].total);\n                }\n                return totalSum;\n            }\n        },\n        methods: {\n            sellersPageClick() {\n                this.$store.dispatch('SET_HEADER_TEXT', 'Поставщики');\n            },\n\n            checkout() {\n               this.orderChecked = true;\n               this.$store.dispatch('CHECKOUT', this.orders);\n            },\n\n            closePopup() {\n                this.orderChecked = false;\n            },\n\n            removeFromCart(index) {\n                this.$store.dispatch('REMOVE_FROM_CART', index);\n            },\n\n            plusQty(index) {\n                this.$store.dispatch('INCREMENT_PRODUCT_QTY', index);\n            },\n\n            minusQty(index) {\n                this.$store.dispatch('DECREMENT_PRODUCT_QTY', index);\n            }\n        },\n        created() {\n            this.orderChecked = false;\n        },\n\n        mounted() {\n            this.$store.dispatch('SET_HEADER_TEXT', 'Мои заказы');\n        },\n    }\n</script>\n\n<style scoped>\n    .a-order-list {\n        max-width: 800px;\n        margin: 50px auto;\n    }\n\n    .list_title {\n        margin: 0 -10px 30px -10px;\n        padding: 10px 0;\n        font-size: 25px;\n    }\n\n    .empty-order-list-p {\n        margin-bottom: 10px;\n    }\n\n    .empty-order-list a p {\n        max-width: 100px;\n        margin: 0 auto;\n    }\n    \n    .order-total {\n        bottom: 61px;\n        font-size: 20px;\n        padding: 10px 0;\n        margin: 0 auto;\n        font-weight: bold;\n        display: flex;\n        align-items: center;\n        justify-content: center;\n        border-bottom: solid 1px #fff;\n    }\n\n    .total-sum {\n        display: flex;\n    }\n\n    .total-sum_confirm {\n        margin-left: 30px;\n        box-shadow: 0 0 5px 0 #404040;\n    }\n    .total-sum_confirm:active {\n        box-shadow: inset 0 0 5px 0 #50753d;\n    }\n\n    .order-total span {\n        margin-left: 10px;\n    }\n\n    .empty-order-list {\n        padding-top: 35px;\n    }\n\n    .thankyou-popup {\n        position: absolute;\n        width: 100%;\n        height: 200px;\n        padding-top: 100px;\n        top: 0;\n        bottom: 0;\n        left: 0;\n        background: #fff;\n        z-index: 2;\n    }\n\n    .ok {\n        max-width: 150px;\n        margin: 0 auto;\n    }\n</style>"]}]}