             orderChecked: false\n            }\n        },\n        computed: {\n            orders() {\n                return this.$store.state.cart;\n            },\n\n            totalSum() {\n                let totalSum = 0;\n                for (let i = 0; i< this.orders.length; i++) {\n                    totalSum += parseInt(this.orders[i].total);\n                }\n                return totalSum;\n            }\n        },\n        methods: {\n            sellersPageClick() {\n                this.$store.dispatch('SET_HEADER_TEXT', 'Поставщики');\n            },\n\n            checkout() {\n               this.orderChecked = true;\n               this.$store.dispatch('CHECKOUT', this.orders);\n            },\n\n            closePopup() {\n                this.orderChecked = false;\n            },\n\n            removeFromCart(index) {\n                this.$store.dispatch('REMOVE_FROM_CART', index);\n            },\n\n            plusQty(index) {\n                this.$store.dispatch('INCREMENT_PRODUCT_QTY', index);\n            },\n\n            minusQty(index) {\n                this.$store.dispatch('DECREMENT_PRODUCT_QTY', index);\n            }\n        },\n        created() {\n            this.orderChecked = false;\n        },\n\n        mounted() {\n            this.$store.dispatch('SET_HEADER_TEXT', 'Мои заказы');\n        },\n    }\n</script>\n\n<style scoped>\n    .a-order-list {\n        max-width: 800px;\n        margin: 50px auto;\n    }\n\n    .list_title {\n        margin: 0 -10px 30px -10px;\n        padding: 10px 0;\n        font-size: 25px;\n    }\n\n    .empty-order-list-p {\n        margin-bottom: 10px;\n    }\n\n    .empty-order-list a p {\n        max-width: 100px;\n        margin: 0 auto;\n    }\n    \n    .order-total {\n        bottom: 61px;\n        font-size: 20px;\n        padding: 10px 0;\n        margin: 0 auto;\n        font-weight: bold;\n        display: flex;\n        align-items: center;\n        justify-content: center;\n        border-bottom: solid 1px #fff;\n    }\n\n    .total-sum {\n        display: flex;\n    }\n\n    .total-sum_confirm {\n        margin-left: 30px;\n        box-shadow: 0 0 5px 0 #404040;\n    }\n    .total-sum_confirm:active {\n        box-shadow: inset 0 0 5px 0 #50753d;\n    }\n\n    .order-total span {\n        margin-left: 10px;\n    }\n\n    .empty-order-list {\n        padding-top: 35px;\n    }\n\n    .thankyou-popup {\n        position: absolute;\n        width: 100%;\n        height: 200px;\n        padding-top: 100px;\n        top: 0;\n        bottom: 0;\n        left: 0;\n        background: #fff;\n        z-index: 2;\n    }\n\n    .ok {\n        max-width: 150px;\n        margin: 0 auto;\n    }\n</style>"],"sourceRoot":"src/components"}]}